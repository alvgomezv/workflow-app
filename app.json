{
  "expo": {
    "name": "workflow-app",
    "slug": "workflow-app",
    "scheme": "workflow-app-scheme",
    "description": "An app to create and manage workflows",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/workflow_icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/workflow_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/workflow_icon.png"
    },
    "plugins": ["expo-router"]
  }
}
